<h2 class="mb-5"><%- item.title %> </h2>

<% item.lists.forEach(element => { %>

<div class="d-flex flex-column flex-md-row justify-content-between mb-5">
  <div class="flex-grow-1">
    <h3 class="mb-0"><%- element.name %> </h3>
    <div class="subheading mb-3"><%- element.responsibility %></div>
    <p><%- element.content %></p>
  </div>
  <div class="flex-shrink-0"><span class="text-primary"><%- element.date %></span></div>
</div>

<% }) %>