<!-- Navigation -->
<!-- <nav class="navbar navbar-default navbar-custom ">navbar-fixed-top navbar-visibled -->
<nav class="navbar navbar-default navbar-custom navbar-static-top navbar-fixed-top is-fixed is-visible">
    <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header page-scroll">
            <button type="button" class="navbar-toggle" data-toggle="collapse"
                data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>

            </button>
            <a class="navbar-brand" href="<%- config.root %>"><%- theme.menu_title || config.title || "" %></a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div id="meblog_navbar">
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right ">                
                    <% for (var i in theme.menu){ %>
                        <li class="nav-item hvr-underline-from-center hvr-float-shadow">
                            <a href="<%- theme.menu[i].url ? url_for(theme.menu[i].url) : url_for(theme.menu[i]) %>">
    
                                <% if (theme.menu[i].icon) { %>
                                <i class="fa fa-<%= theme.menu[i].icon %> fa-stack-2x">
                                    <% } %>
    
                                    <%= i %>
                                </i>
                            </a>
                        </li>
                    <% } %>
    
                    <% if(theme.baidu_search.enable){ %>
    
                        <li class="nav-item search-input">
                            <form class="navbar-form search bar7" action="" method="get" accept-charset="utf-8">
                                <input type="text" id="local-search-input" autocomplete="off" name="q" maxlength="50"
                                    placeholder="search" />
                                <button type="button"></button>
                                <ul class="search-result-list hidden" id="local-search-result"> </ul>
                            </form>
                        </li>
                    <% } %>

                </ul>
            </div>

        </div>
        
        <!-- /.navbar-collapse -->
    </div>
    <!-- /.container -->
    
</nav>




